-44</Citation>\n                <ArticleIdList>\n                    <ArticleId IdType="pubmed">25122517</ArticleId>\n                </ArticleIdList>\n            </Reference>\n        </ReferenceList>\n    </PubmedData>\n</PubmedArticle>\n\n</PubmedArticleSet>'